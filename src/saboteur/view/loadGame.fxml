<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="786.0" prefWidth="823.0" style="-fx-background-color: rgba(0,0,0,0.7);">
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <children>
            <ImageView fitHeight="99.0" fitWidth="844.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../resources/chargerPartieLogo.png" />
               </image>
               <VBox.margin>
                  <Insets bottom="40.0" top="20.0" />
               </VBox.margin>
            </ImageView>
            <ScrollPane prefHeight="371.0" prefWidth="691.0" style="-fx-background-color: transparent;">
               <VBox.margin>
                  <Insets bottom="25.0" left="40.0" right="40.0" top="25.0" />
               </VBox.margin>
               <content>
                  <VBox fx:id="loadableGameContainer" prefHeight="500.0" prefWidth="748.0" style="-fx-background-color: rgba(0,0,0,0.7);">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <padding>
                        <Insets bottom="10.0" left="120.0" right="10.0" top="10.0" />
                     </padding>
                  </VBox>
               </content>
            </ScrollPane>
            <AnchorPane>
               <children>
                  <Button alignment="CENTER" layoutX="125.0" layoutY="3.0" mnemonicParsing="false" onAction="#backButtonAction" prefHeight="44.0" prefWidth="135.0" text="Annuler" AnchorPane.leftAnchor="125.0">
                     <font>
                        <Font size="20.0" />
                     </font></Button>
                  <Button alignment="CENTER_RIGHT" layoutX="586.0" layoutY="2.0" mnemonicParsing="false" onAction="#startLoadableGameButtonAction" text="Commencer" AnchorPane.rightAnchor="123.0">
                     <font>
                        <Font size="20.0" />
                     </font></Button>
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" top="50.0" />
               </VBox.margin>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</HBox>
