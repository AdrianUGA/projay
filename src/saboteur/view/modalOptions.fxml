<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" opacity="0.7" prefHeight="400.0" prefWidth="300.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="54.0" fitWidth="275.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../resources/OptionsLogo.png" />
         </image>
         <VBox.margin>
            <Insets bottom="40.0" top="20.0" />
         </VBox.margin>
      </ImageView>
      <Pane prefHeight="200.0" prefWidth="200.0" />
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Button fx:id="cancelOptionModalButton" alignment="CENTER" layoutX="24.0" layoutY="80.0" mnemonicParsing="false" onMouseClicked="#cancelOptionModalButtonAction" prefHeight="25.0" prefWidth="81.0" text="Annuler" AnchorPane.leftAnchor="24.0" />
            <Button fx:id="okOptionsModalButton" alignment="CENTER" contentDisplay="CENTER" layoutX="205.0" layoutY="80.0" mnemonicParsing="false" onMouseClicked="#okOptionModalButtonAction" prefHeight="25.0" prefWidth="81.0" text="OK" AnchorPane.rightAnchor="24.0" />
         </children>
      </AnchorPane>
   </children>
</VBox>
